{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": "\\.jsx?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "better-docs": {
      "name": "My React components"
    }
  },
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "theme": "dark"
    }
  }
}
